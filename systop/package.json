{
  "name": "sys-top",
  "productName": "SysTop",
  "version": "1.0.0",
  "description": "Monitor your system CPU",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SysTop\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "keywords": [
    "Electron"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solygambas/electron-image-shrink.git"
  },
  "author": "solygambas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solygambas/electron-image-shrink/issues"
  },
  "homepage": "https://github.com/solygambas/electron-image-shrink#readme",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "node-os-utils": "^1.3.2"
  }
}
